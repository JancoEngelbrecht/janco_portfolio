{
  "mimeTypes": {
    ".json": "application/json"
  },
  "routes": [
    {
      "route": "/static/*",
      "headers": {
        "cache-control": "max-age=3600"
      }
    },
    {
      "route": "/manifest.json",
      "serve": "/manifest.json"
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ]
}